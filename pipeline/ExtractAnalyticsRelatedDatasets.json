								}
							},
							{
								"source": {
									"path": "['group_name']"
								},
								"sink": {
									"name": "group_name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['page_views']"
								},
								"sink": {
									"name": "page_views",
									"type": "Int64"
								}
							},
							{
								"source": {
									"path": "['visits']"
								},
								"sink": {
									"name": "visits",
									"type": "Int64"
								}
							}
						],
						"collectionReference": "$['items']"
					}
				},
				"inputs": [
					{
						"referenceName": "AnalyticsGroupsRestDataset",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AnalyticsGroupsCsvDataset",
						"type": "DatasetReference"
					}
