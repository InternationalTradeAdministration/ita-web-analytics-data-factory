{
	"name": "AnalyticsHistoricalMetricsDataset",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorageLinkedService",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"GroupId": {
				"type": "string"
			},
			"Period": {
				"type": "string"
			},
			"FileName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Json",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().FileName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "analytics_historical_metrics/@{dataset().GroupId}/@{dataset().Period}",
					"type": "Expression"
				},
				"container": "ita-web-analytics"
			}
		},
		"schema": {}
	}
}